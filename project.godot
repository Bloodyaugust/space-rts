[display]

window/size/width=1920
window/size/height=1080

[editor_plugins]

enabled=PoolStringArray( "godot-quick-find", "gut" )

[gui]

timers/tooltip_delay_sec=0.1

[rendering]

environment/default_environment="res://default_env.tres"
